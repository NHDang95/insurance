<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <h2 class="page-header title"><%= t ".user_list" %></h2>
      </div>
    </div>
    <table class="table table-bordered mt-5" >
      <thead>
        <tr class="bold">
          <th><%= t "shared.sign_up.name" %></th>
          <th><%= t "shared.sign_up.email" %></th>          
          <th><%= t "shared.sign_up.phone" %></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr style="text-align: center;">
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.phone %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <div class="text-center">
      <%= will_paginate @users, :previous_label => t("shared.previous"), :next_label => t("shared.next") %>
    </div>
  </div>
</div>
