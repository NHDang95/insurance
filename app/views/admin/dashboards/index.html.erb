<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12 mt-30">
      </div>
    </div>
  </div>
  <div class="col-lg-offset-2 col-lg-4 col-md-6">
    <div class="panel panel-yellow">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-legal fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= @products %></div>
            <div><%= t "shared.insurances" %></div>
          </div>
        </div>
      </div>
      <a href="#">
        <div class="panel-footer">
          <span class="pull-left">
            <%= link_to admin_products_path do %>
            <%= t "static_pages.home.more_detail" %>
            <i class="fa fa-arrow-circle-right"></i>
            <% end %>
          </span>
          <div class="clearfix"></div>
        </div>
      </a>
    </div>
  </div>
  <div class="col-lg-4 col-md-6">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-user fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= @users %></div>
            <div><%= t "shared.users" %></div>
          </div>
        </div>
      </div>
      <a href="#">
        <div class="panel-footer">
          <span class="pull-left">
            <%= link_to admin_users_path do %>
            <%= t "static_pages.home.more_detail" %>
            <i class="fa fa-arrow-circle-right"></i>
            <% end %>
          </span>
          <div class="clearfix"></div>
        </div>
      </a>
    </div>
  </div>
  <div class="col-lg-offset-2 col-lg-4 col-md-6">
    <div class="panel panel-red">
      <div class="panel-heading">
        <div class="row">
          <div class="col-xs-3">
            <i class="fa fa-automobile fa-5x"></i>
          </div>
          <div class="col-xs-9 text-right">
            <div class="huge"><%= @brands %></div>
            <div><%= t "shared.brands" %></div>
          </div>
        </div>
      </div>
      <a href="#">
        <div class="panel-footer">
          <span class="pull-left">
            <%= link_to admin_brands_path do %>
            <%= t "static_pages.home.more_detail" %>
            <i class="fa fa-arrow-circle-right"></i>
            <% end %>
          </span>
          <div class="clearfix"></div>
        </div>
      </a>
    </div>
  </div>  <div class="col-lg-4 col-md-6">
  <div class="panel panel-green">
    <div class="panel-heading">
      <div class="row">
        <div class="col-xs-3">
          <i class="fa fa-feed fa-5x"></i>
        </div>
        <div class="col-xs-9 text-right">
          <div class="huge"><%= @articles %></div>
          <div><%= t "shared.articles" %></div>
        </div>
      </div>
    </div>
    <a href="#">
      <div class="panel-footer">
        <span class="pull-left">
          <%= link_to admin_articles_path do %>
          <%= t "static_pages.home.more_detail" %>
          <i class="fa fa-arrow-circle-right"></i>
          <% end %>
        </span>
        <div class="clearfix"></div>
      </div>
    </a>
  </div>
</div>
<div class="col-lg-4 col-md-6">
</div>
</div>