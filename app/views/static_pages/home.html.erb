<div class="clearfix"></div>
<div class="home-slider">
  <div class="container">
    <div id="sequence">
      <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Wrapper for slides -->
        <div class="carousel-inner">
          <div class="item active center">
            <%= image_tag "mazda6.png" %>
            <div class="carousel-caption">
              <h3 style="color: red;">Mazda 6</h3>
            </div>
          </div>
          <div class="item center">
            <%= image_tag "santafe.png" %>
            <div class="carousel-caption">
              <h3 style="color: red;">Santa Fe</h3>
            </div>
          </div>
          <div class="item center">
            <%= image_tag "vios.png" %>
            <div class="carousel-caption">
              <h3 style="color: red;">Vios</h3>
            </div>
          </div>
        </div>
        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
          <a class="left carousel-control" href="#myCarousel" data-slide="prev">
          </a>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
          <a class="right carousel-control" href="#myCarousel" data-slide="next">
          </a>
        </a>
      </div>
    </div>
  </div>
</div>  <!-- end home-slider -->
  <div class="clearfix"></div>
  <div class="container_fullwidth">
    <div class="container">
      <div class="hot-products">
        <section>
          <div class="row">
            <div class="jumbotron full-text-title" style="background-color: #DEE4E7;">
              <div class="row">
                <div class="col-sm-12">
                  <div class="table">
                    <div class="table-cell">
                      <h2 class="text-center">Tiêu đề</h2>
                      <p class="text-center">D&H Việt Nam tự hào tiên phong mang đến trải nghiệm hoàn toàn mới về bộ hợp đồng bảo hiểm theo tiêu chuẩn dễ hiểu để khách hàng biết được quyền lợi của mình một cách dễ dàng.</p>
                    </div>
                  </div>
                  <div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
        <%= render partial: "shared/article"%>
      </div> <!-- end hot-product -->
      <div class="clearfix"></div>
    </div> <!-- end container(213) -->
  </div> <!-- end container-fullwidth(212) -->