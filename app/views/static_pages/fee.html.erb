<div class="clearfix"></div>
<div class="container_fullwidth fee-page">
  <div class="banner-page">
    <div class="container">
      <h2 class="text-center"><b><%= t(".fee_title").upcase %></b></h2>
      <hr />
      <p class="desc-title"></p>
      <div class="breadcrumb" typeof="BreadcrumbList" vocab="http://schema.org/">
        <!-- Breadcrumb NavXT 6.4.0 -->
        <span property="itemListElement" typeof="ListItem">
          <a property="item" typeof="WebPage" title="Trang chủ" href="/" class="home" itemprop="item" data-wpel-link="internal"><span property="name">Trang chủ</span></a>
          <meta property="position" content="1"></span> <i class="fa fa-angle-double-right"></i> <span property="itemListElement" typeof="ListItem"><a property="item" typeof="WebPage" title="Go to the Hỗ trợ category archives." href="/ho-tro/" class="taxonomy category" itemprop="item" data-wpel-link="internal"><span property="name">Hỗ trợ</span></a>
          <meta property="position" content="2"></span> <i class="fa fa-angle-double-right"></i> <span property="itemListElement" typeof="ListItem"><span property="name" itemprop="item"><%= t(".fee_title") =%></span>
          <meta property="position" content="3"></span>
      </div>
    </div>
  </div>
  <div class="hot-products">
    <div class="container">
      <div class="row">
        <ul>
          <li class="col-xs-4">
            <a class="fa fa-bank fa-5x"></a>
            <em><%= t".bank" %></em>
          </li>
          <li class="col-xs-4">
            <a class="fa fa-universal-access fa-5x"></a>
            <em><%= t".service" %></em>
          </li>
          <li class="col-xs-4">
            <a class="fa fa-handshake-o fa-5x"></a>
            <em><%= t".consult" %></em>
          </li>
        </ul>
      </div>
      <div class="row content">
        <div class="col-md-12">
          <div class="clearfix"></div>
          <section>
            <h1 class="text-center"><b><%= t".caution" %></b></h1>
            <p><%= t".description1" %></p>
            <p><%= t".description2" %></p>
            <p><%= t".description3" %></p>
          </section>
          <div class="clearfix"></div>
          <div>
            <h2><b><%= t".info" %></b></h2>
            <div class="contact">
              <i class="fa fa-mobile fa-5x"></i>
              <h3>09 8601 9195</h3>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>